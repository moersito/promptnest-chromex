{
    "manifest_version": 3,
    "name": "PromptNest",
    "description": "PromptNest can collects , analyze many prompt text for any AI chat platforms in the world.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "identity"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "icons": {
      "16": "icons/PromptNest_16.png",
      "48": "icons/PromptNest_48.png",
      "128": "icons/PromptNest_128.png"
    }
  }
  